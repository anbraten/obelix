{
  "name": "obelix",
  "version": "1.0.0",
  "description": "This wep app allows you to rent boats.",
  "repository": "https://github.com/anbraten/obelix",
  "author": "Anbraten",
  "private": true,
  "scripts": {
    "start": "dotenv -- pnpm multi --color --filter @obelix/seagull --filter @obelix/backend run start",
    "start:seagull": "dotenv -- pnpm run --filter @obelix/seagull start",
    "start:backend": "dotenv -- pnpm run --filter @obelix/backend start",
    "build": "pnpm run --filter @obelix/seagull build",
    "lint": "pnpm run --filter \"@obelix/*\" lint",
    "lint:fix": "pnpm run --filter \"@obelix/*\" lint --fix"
  },
  "dependencies": {
    "concurrently": "5.3.0",
    "dotenv-cli": "4.0.0"
  }
}
