{
  "name": "obelix",
  "version": "1.0.0",
  "description": "This wep app allows you to rent boats.",
  "repository": "https://github.com/anbraten/obelix",
  "author": "Anbraten",
  "private": true,
  "scripts": {
    "start": "dotenv concurrently \"yarn start:seagull\" \"yarn start:backend\"",
    "start:app": "dotenv yarn workspace @obelix/seagull run start",
    "start:backend": "dotenv yarn workspace @obelix/backend run start",
    "build": "yarn workspace @obelix/app run build",
    "lint": "yarn workspaces run lint",
    "lint:fix": "yarn workspaces run lint --fix"
  },
  "dependencies": {
    "concurrently": "5.3.0",
    "dotenv-cli": "4.0.0"
  }
}
