{
  "name": "obelix",
  "version": "1.0.0",
  "description": "This wep app allows you to rent boats.",
  "repository": "https://github.com/anbraten/obelix",
  "author": "Anbraten",
  "private": true,
  "workspaces": ["packages/*"],
  "scripts": {
    "dev": "yarn workspace @obelix/backend run dev & yarn workspace @obelix/app run dev",
    "build": "yarn workspace @obelix/app run build",
    "build:preview": "yarn generate:config && yarn build",
    "lint": "yarn workspaces run lint",
    "lint:fix": "yarn workspaces run lint --fix",
    "generate:config": "./docker/set-env-vars.sh ./spa/public/env-config.js"
  },
  "resolutions": {
    "**/workbox-webpack-plugin": "5.0.0"
  }
}
