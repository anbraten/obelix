{
  "host": "localhost",
  "port": 4000,
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "authentication": {
    "entity": "user",
    "service": "users",
    "secret": "JWT_SECRET",
    "authStrategies": [
      "jwt",
      "keycloak"
    ],
    "jwtOptions": {
      "header": {
        "typ": "access"
      },
      "audience": "api",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "oauth": {
      "redirect": "OAUTH_REDIRECT_URL",
      "keycloak": {
        "secret": "KEYCLOAK_SECRET",
        "client_id": "KEYCLOAK_CLIENT",
        "subdomain": "KEYCLOAK_SUBDOMAIN",
        "authorize_url": "https://[subdomain]/protocol/openid-connect/auth",
        "access_url": "https://[subdomain]/protocol/openid-connect/token",
        "profile_url": "https://[subdomain]/protocol/openid-connect/userinfo",
        "oauth": 2,
        "nonce": true,
        "scope_delimiter": " "
      }
    }
  },
  "mongodb": "MONGODB_URI"
}
